{
  "name": "vue-ac-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clean:android:app": "rm -rf ./../android/app/src/main/assets/widget/**",
    "copy:android:app": "cp -rf dist/* ./../android/app/src/main/assets/widget/ && rm -rf ./../android/app/src/main/assets/widget/web_adapter",
    "android:app": "yarn build && yarn clean:android:app && yarn copy:android:app",
    "clean:ios:app": "rm -rf ./../ios/app/widget/**",
    "copy:ios:app": "cp -rf dist/* ./../ios/app/widget/ && rm -rf ./../ios/app/widget/web_adapter",
    "ios:app": "yarn build && yarn clean:ios:app && yarn copy:ios:app"
  },
  "dependencies": {
    "echarts": "^4.1.0",
    "vue": "^2.6.6",
    "vue-echarts": "^4.0.1",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
